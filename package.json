{
  "name": "shopzone",
  "version": "1.0.0",
  "description": "MERN stack eCommerce shop",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guy-saloniki/shopzone.git"
  },
  "author": "Guy Saloniki",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/guy-saloniki/shopzone/issues"
  },
  "homepage": "https://github.com/guy-saloniki/shopzone#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.18.2",
    "mongoose": "^7.1.0"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.22"
  }
}
